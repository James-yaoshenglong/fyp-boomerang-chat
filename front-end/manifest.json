{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*",
        "<all_urls>",
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "index.html"
  }